{
  "devDependencies": {
    "@ethersproject/abi": "5.4.0",
    "@ethersproject/bytes": "5.0.0",
    "@ethersproject/providers": "5.0.0",
    "@openzeppelin/contracts": "4.1.0",
    "@openzeppelin/contracts-upgradeable": "4.1.0",
    "@openzeppelin/hardhat-upgrades": "1.8.2",
    "@openzeppelin/upgrades-core": "1.7.6",
    "@typechain/ethers-v5": "7.0.1",
    "@typechain/hardhat": "2.0.2",
    "@types/node": "15.12.2",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-etherscan": "2.1.1",
    "dotenv": "10.0.0",
    "solidity-coverage": "0.7.16",
    "eslint": "7.28.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.3.1",
    "prettier-plugin-solidity": "1.0.0-beta.13",
    "solhint": "3.3.6",
    "solhint-plugin-prettier": "0.0.5",
    "ts-node": "10.0.0",
    "typechain": "5.0.0",
    "typescript": "4.3.2",
    "ethers": "5.4.1",
    "hardhat": "2.6.4",
    "logdown": "3.3.1",
    "ts-generator": "0.1.1"
  },
  "scripts": {
    "postinstall": "hardhat typechain",
    "typechain": "hardhat typechain",
    "build": "yarn run clean && yarn run compile",
    "clean": "hardhat clean",
    "compile": "hardhat compile",
    "coverage": "yarn run build && hardhat coverage --temp artifacts --network hardhat",
    "hardhat": "hardhat",
    "test": "hardhat test --show-stack-traces",
    "fix": "yarn run fix:contracts && yarn run fix:ts",
    "fix:contracts": "prettier --write ./contracts/**/*.sol",
    "fix:ts": "eslint . --ext .ts --fix",
    "lint": "yarn run lint:contracts && yarn run lint:ts",
    "lint:contracts": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "lint:ts": "eslint . --ext .ts"
  },
  "name": "zauction",
  "version": "0.1.0",
  "license": "MIT"
}
